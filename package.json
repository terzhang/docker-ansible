{
  "name": "node-docker",
  "version": "0.0.1-alpha",
  "main": "app.ts",
  "scripts": {
    "start": "npm run build && node build/app.js",
    "build": "tsc",
    "watch": "tsc-watch --onSuccess \"node build/app.js\"",
    "dev": "tsc && nodemon ./build/app.js"
  },
  "devDependencies": {
    "@types/node": "^17.0.18",
    "nodemon": "^2.0.15",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.5.5"
  }
}